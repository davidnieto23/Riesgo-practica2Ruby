
<p id="notice"><%= notice %></p>

<h1>Postulate para donar sangre</h1>



<table>
  <thead>
    <tr>
      <th>Sangre</th>
      <th>Comentario</th>
      <th>Solved</th>
      <th>Usuario</th>
      <th>Usuarios</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @publicaciones.each do |post| %>
      <tr>
        <td><%= post.sangre %></td>
        <td><%= post.comentario %></td>
        <td><%= post.solved %></td>
        <td><%= post.usuario_id %></td>
        <td><%= post.to_json %></td>
        <td><%= link_to 'Edit', posts_show_path(post) %></td>
  <div class="actions">
  </div>
      </tr>
    <% end %>
  </tbody>
</table>

<br>